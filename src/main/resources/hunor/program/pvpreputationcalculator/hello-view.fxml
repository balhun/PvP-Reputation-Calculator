                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onClickGradeIPlus" prefHeight="25.0" prefWidth="25.0" text="+" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="170.0" prefWidth="250.0" spacing="5.0">
                     <children>
                        <Label layoutX="40.0" layoutY="14.0" prefHeight="25.0" prefWidth="175.0" text="Hourglass Lowered">
                           <font>
                              <Font size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <CheckBox fx:id="cbHourglassLowered" mnemonicParsing="false" onAction="#onClickHourglassLowered" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER_RIGHT">
               <children>
                  <Pane>
                     <children>
                        <ImageView fx:id="ImgProgress" fitHeight="243.0" fitWidth="241.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@Images/progression-bg-servant.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="lbLevel" alignment="CENTER" prefHeight="243.0" prefWidth="241.0" text="1.000" textFill="WHITE">
                           <font>
                              <Font size="40.0" />
                           </font>
                        </Label>
                     </children>
                  </Pane>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <VBox alignment="CENTER">
         <children>
            <Label fx:id="lbXpGain" text="Xp gained:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label fx:id="lbLevelGain" text="Level Gained:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
