                        <Label layoutX="40.0" layoutY="14.0" prefHeight="25.0" prefWidth="140.0" text="Grade II">
                           <font>
                              <Font size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <Button mnemonicParsing="false" onAction="#onClickGradeIIMinus" prefHeight="25.0" prefWidth="25.0" text="-" />
