                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onClickGradeIIPlus" prefHeight="25.0" prefWidth="25.0" text="+" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="140.0" spacing="5.0">
                     <children>
                        <Label layoutX="40.0" layoutY="14.0" prefHeight="25.0" prefWidth="140.0" text="Grade I">
                           <font>
                              <Font size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <Button mnemonicParsing="false" onAction="#onClickGradeIMinus" prefHeight="25.0" prefWidth="25.0" text="-" />
                        <Label fx:id="lbGradeI" text="0">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
