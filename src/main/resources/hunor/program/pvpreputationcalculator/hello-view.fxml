                        <Label layoutX="40.0" layoutY="14.0" prefHeight="25.0" prefWidth="140.0" text="Grade III">
                           <font>
                              <Font size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <Button mnemonicParsing="false" onAction="#onClickGradeIIIMinus" prefHeight="25.0" prefWidth="25.0" text="-" />
                        <Label fx:id="lbGradeIII" text="0">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin></Label>
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onClickGradeIIIPlus" prefHeight="25.0" prefWidth="25.0" text="+" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="110.0" spacing="5.0">
