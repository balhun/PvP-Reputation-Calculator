<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hunor.program.pvpreputationcalculator.Script">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Faction">
            <items>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false">
                     <content>
                        <ChoiceBox fx:id="choiceBox" maxWidth="1.7976931348623157E308" />
                     </content>
                  </CustomMenuItem>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Your Level">
            <items>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <TextField fx:id="tfLevelXp" alignment="CENTER" onAction="#onLevelSet" text="1000" />
                     </content>
                  </CustomMenuItem>
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Goal">
               <items>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Unspecified Action">
                     <content>
                        <TextField fx:id="tfGoalLevel" alignment="CENTER" onAction="#onSetGoalLevel" text="1000" />
                     </content>
                  </CustomMenuItem>
               </items>
            </Menu>
            <Menu mnemonicParsing="false">
               <graphic>
                  <Label fx:id="lbXpRequired" alignment="CENTER" maxWidth="1.7976931348623157E308" text="0" />
               </graphic>
            </Menu>
        </menus>
      </MenuBar>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <VBox spacing="5.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <children>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" spacing="5.0">
                     <children>
                        <Label maxWidth="1.7976931348623157E308" text="What level are you?" HBox.hgrow="ALWAYS" />
                        <TextField fx:id="tfYourLevel" alignment="CENTER" focusTraversable="false" onAction="#onUseYourLevel" prefHeight="25.0" prefWidth="45.0" text="1000">
                           <HBox.margin>
                              <Insets right="31.0" />
                           </HBox.margin></TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <Label layoutX="40.0" layoutY="14.0" prefHeight="25.0" prefWidth="140.0" text="Battles Won">
                           <font>
                              <Font size="16.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="4.0" right="5.0" />
                           </HBox.margin>
                        </Label>
                        <Button mnemonicParsing="false" onAction="#onClickBattlesWonMinus" prefHeight="25.0" prefWidth="25.0" text="-" />
                        <Label fx:id="lbBattlesWon" text="0">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin></Label>
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onClickBattlesWonPlus" prefHeight="25.0" prefWidth="25.0" text="+" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="15.0" layoutY="15.0" spacing="5.0">
